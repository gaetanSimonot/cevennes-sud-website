# Google Maps API Key (required for maps and geocoding)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenAI API Key (required for AI extraction in Artefact IA)
OPENAI_API_KEY=sk_your_openai_api_key_here
